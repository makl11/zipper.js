{
  "name": "@makl11/zipper.js",
  "type": "module",
  "version": "0.7.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": "./dist/index.iife.js",
  "types": "./dist/index.d.cts",
  "repository": {
    "type": "git",
    "url": "https://github.com/makl11/zipper.js.git"
  },
  "keywords": [
    "zip",
    "stream"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepack": "npm run build",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "crc": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "files": [
    "dist/*",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./node-compat": {
      "import": "./dist/node-compat.js",
      "require": "./dist/node-compat.cjs"
    },
    "./package.json": "./package.json"
  }
}