  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "typecheck": "tsc --noEmit -p ."
